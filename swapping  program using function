#include<stdio.h>
void swap(int*a,int*b);
int main(){
int m=10;
int n=20;
printf("First number m:%d\n",m);
printf("Second number n:%d\n",n);

swap(&m,&n);
printf("After swapping: \n\n");
printf("First number m:%d\n",m);
printf("Second number n:%d",n);
return 0;
}
void swap(int*a,int*b)
{
    int temp;
    temp=*a;
    *a=*b;
    *b=temp;
    
}
