#include <stdio.h>
#include<string.h>
int main() {
    char str[20];
    
    printf("\nEnter a string ");
    scanf("%s",str);
    char c;
    c=str[0];
    int count=0;
    int len=strlen(str);
    for(int j=0;j<len;j++)
    {
        c=str[j];
        count=0;
    for(int i=0;i<len;i++)
    {
        if(str[i]==c)
        count++;
     }
     printf("\n%c is %d",c,count);
    }
    
    return 0;
}
