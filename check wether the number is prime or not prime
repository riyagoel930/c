#include <stdio.h>
#include<math.h>
int main() {
    // Write C code here
    int i,num,flag=0;
    printf("Enter the number: ");
    scanf("%d",&num);
    for(i=2;i<=sqrt(num);i++)
    {
        if(num%i==0)
        flag=1;
        break;
    }
    if(flag==1)
    {
        printf("Number is not prime!");
    }
    else
    {
    printf("Number is not prime!");
    }
return 0;
}
