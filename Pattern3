#include <stdio.h>  
#include<conio.h>
void main()
{ 
    
    for(int i=1;i<=8;i++) 
    {  
        for(int j=1;j<=8;j++) 
        {  
            if(i==1 ||i==8||j==1||j==8)
            {  
            printf("*");  
            }  
            else  
            printf(" ");  
        }  
        printf("\n");  
    }  
      
      
    getch();
}

//

output:

********
*      *
*      *
*      *
*      *
*      *
*      *
********

//
