#include <stdio.h>  
#include<conio.h>
void main()
{ 
    
   for(int i=1;i<=8;i++)
    {  
        for(int j=1;j<i;j++)
        {  
            printf(" ");  
        }  
        for(int k=1;k<=8;k++) 
        {  
            printf("*");  
        }  
        printf("\n");  
    }  
      
      
    getch();
}


//
outpur:
********
 ********
  ********
   ********
    ********
     ********
      ********
       ********
       
       
       
       //
